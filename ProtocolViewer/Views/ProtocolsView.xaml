                                    <ListBox Height="150" ItemsSource="{Binding CustomModelHeaders, UpdateSourceTrigger=PropertyChanged}" 
                             SelectedIndex="{Binding CustomSelectedIndex, UpdateSourceTrigger=PropertyChanged}"
                             Style="{DynamicResource SimpleListBox}" SelectionChanged="ListBox_SelectionChanged" KeyDown="ListBox_KeyDown"
                             ItemContainerStyle="{DynamicResource SimpleListBoxItemNoCheck}">
                                        <ListBox.ContextMenu>
                                            <ContextMenu Template="{DynamicResource SimpleContextMenu}" 
                                         ItemContainerStyle="{DynamicResource SimpleContextMenuItem}">
                                                <MenuItem Header="Удалить" 
                                          Command="{Binding DeleteSelectedCustomItemCommand, UpdateSourceTrigger=PropertyChanged}"/>
                                                <MenuItem Header="Удалить все" 
                                          Command="{Binding ClearCustomCollectionCommand, UpdateSourceTrigger=PropertyChanged}"/>
                                            </ContextMenu>
                                        </ListBox.ContextMenu>
                                    </ListBox>
                                </Border>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <Button Grid.Column="0" Content="Удалить выделенный элемент"
                            Command="{Binding DeleteSelectedCustomItemCommand}" Style="{DynamicResource SimpleButton}"
                            Width="auto" Height="25" Margin="0,3,0,3"/>
                                    <Button Grid.Column="1" Content="Очистить коллекцию"
                            Command="{Binding ClearCustomCollectionCommand}" Style="{DynamicResource SimpleButton}"
                            Width="auto" Height="25" Margin="5,3,0,3"/>
                                </Grid>
                                <Grid ShowGridLines="False">
                                    <Grid.RowDefinitions>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                        <RowDefinition/>
                                    </Grid.RowDefinitions>
                                    <Grid Grid.Row="0" Margin="0,3,0,0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="2.5*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock Grid.Column="0" Grid.Row="1" Style="{DynamicResource TextBlockInStack}"
                                                   Text="Название:" ToolTip="Не преобразуется в код"/>
                                        <TextBox Grid.Column="1" Grid.Row="1" Style="{DynamicResource SimpleTextBox}" 
                                                 Height="20" Text="{Binding NewElementName}" FontSize="14"/>
                                    </Grid>
                                    <Grid Grid.Row="1" Margin="0,3,0,0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="2.5*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock Grid.Column="0" Style="{DynamicResource TextBlockInStack}" Margin="3,3,0,0"
                                                   Text="Уровень:" ToolTip="Если уровень больше предыдущего помещается в Stack" />
                                        <ComboBox Height="30" Width="auto" Grid.Column="1"
                                                  ItemsSource="{Binding AvailableLevels}" Style="{DynamicResource SimpleComboBoxInStackUpMenu}"
                                                  SelectedItem="{Binding SelectedLevel}"/>
                                    </Grid>
                                    <Grid Grid.Row="2" Margin="0,3,0,0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="2.5*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock Grid.Column="0" Grid.Row="2" Style="{DynamicResource TextBlockInStack}" Margin="3,3,0,0"
                                                   Text="Направление:" ToolTip="Направление StackPanel"/>
                                        <ComboBox  Height="30" Width="auto" Grid.Column="1" 
                                                   IsEnabled="{Binding IsSelectedLevelRoot, UpdateSourceTrigger=PropertyChanged}"
                                                   ItemsSource="{Binding AvailableOrientation}" Style="{DynamicResource SimpleComboBoxInStackUpMenu}"
                                                   SelectedItem="{Binding SelectedOrientation}"/>
                                    </Grid>

                                    <Button Grid.ColumnSpan="2" Grid.Row="4" Content="Добавить элемент"
                                            Command="{Binding AddNewElementInCollection}" Style="{DynamicResource SimpleButton}"
                                            Width="auto" Height="25" Margin="0,3,0,3"/>
                                </Grid>
                                <!--#endregion-->
                            </StackPanel>
                        </ScrollViewer>
                        <!--#endregion-->
                    </Grid>
                </Grid>
            </TabItem>
